{
  "extends": [
    "standard",
    "plugin:react/all"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-indent": false,
    "react/jsx-indent-props": false,
    "react/jsx-max-depth": false,
    "react/require-default-props":  false
  }
}
